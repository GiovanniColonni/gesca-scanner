{
  "expo": {
    "name": "gesca-scanner",
    "slug": "gesca-scanner",
    "version": "1.0.0",
    "icon": "./assets/icons/AppIcon.png",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      [
        "expo-build-properties",{ "android": { "usesCleartextTraffic": true } }
      ]
    ],
    "splash": {
      "image": "./assets/icons/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#808080"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.gescascanner",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "13273429-adf5-4da0-bfac-0e289e512d40"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/13273429-adf5-4da0-bfac-0e289e512d40"
    },
    "ios": {}
  }
}
